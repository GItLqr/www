#描述：用户feature偏好 
CREATE TABLE IF NOT EXISTS %(resource_type)s_user_feature_list( 
    `id` bigint(10) NOT NULL AUTO_INCREMENT,
    uid VARCHAR(255) NOT NULL, 
    user_feature_list MEDIUMBLOB, 
    PRIMARY KEY (`id`),
    UNIQUE (uid)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

